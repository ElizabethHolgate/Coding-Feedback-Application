<% layout('layouts/boilerplate')%>
<div class="container mb-3">
    <h3>Task: <%= task.task %></h3>
    <samp>
        <% diff.forEach(d => { %>
            <% if(d.removed) { %>
                <font color="red">
                    <%= d.value %>
                </font>
            <% } else if(!d.added) { %>
                <font color="black">
                    <%= d.value %>
                </font>
            <% } %>
        <% }) %>
            </samp>
    <p><%= feedback %></p>
    <p><%= percent %></p>
</div>
<a class="btn btn-primary" href="/modules/<%= module._id%>">Back To Module</a>